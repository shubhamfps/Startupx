<Window x:Class="Startupx.WarehouseModule.Add_Stock"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Startupx.WarehouseModule"
        mc:Ignorable="d"
        Title="Add_Stock" Height="254.473" Width="339.936"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="txt_stock" HorizontalAlignment="Left" Height="29" Margin="113,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Image HorizontalAlignment="Left" Height="67" Margin="137,13,0,0" VerticalAlignment="Top" Width="64" Source="/Startupx;component/Images/quant_icon.png"/>
        <Button Content="Increase" HorizontalAlignment="Left" Margin="112,143,0,0" VerticalAlignment="Top" Width="123" Height="30" Background="#FF458CA2" Foreground="White" FontWeight="Bold" Click="Button_Click"/>
        <Label Content="+" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="234,98,0,0" FontSize="20" Foreground="#FFF36B13" FontWeight="Bold" Height="35" Width="26"/>
        <Label Content="Stock" HorizontalAlignment="Left" Margin="73,107,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
